programa
{
	
	funcao inicio()                                                    // Exercício 1: Idade daqui 5 anos
	{
		cadeia nome
		escreva ("Digite seu nome: ")
		leia(nome)

		real idade, soma
		escreva("Digite sua idade: ")
		leia(idade)
		soma = idade + 5
		escreva("Daqui a 5 anos você vai ter: ", soma)
	}
}
